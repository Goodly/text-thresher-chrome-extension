  {
  "name": "Text Thresher",
  "version": "0.0.0",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "dist/images/icon-16.png",
    "128": "dist/images/icon-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "19": "dist/images/icon-19.png",
      "38": "dist/images/icon-38.png"
    },
    "default_title": "text-thresher",
    "default_popup": "dist/popup.html"
  },
   "background": {
      "scripts": [
      "dist/scripts/chromereload.js",
      "dist/scripts/background.js"
      ]
   },
  "options_page": "dist/options.html",
  "omnibox": {
    "keyword": "thresher"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "dist/scripts/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "tabs",
    "management",
    "http://*/*",
    "https://*/*"
  ]
}
